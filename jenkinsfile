pipeline{
    agent any        
       }
      stages {
      customWorkspace '/mnt/jenkins1'
        stage('static web') { 
            steps {
                echo "install httpd in slave"
            }
        }
        stage('Install httpd') { 
            steps {
                sh 'sudo yum install httpd -y' 
            }
        }
        stage('status httpd') { 
            steps {
                sh 'sudo service httpd start' 
           }        
    }
}
